{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/desktop-agent/src",
  "monorepo": true,
  "root": "apps/desktop-agent",
  "compilerOptions": {
    "tsConfigPath": "apps/desktop-agent/tsconfig.app.json",
    "webpack": false
  },
  "projects": {
    "desktop-agent": {
      "type": "application",
      "root": "apps/desktop-agent",
      "entryFile": "main",
      "sourceRoot": "apps/desktop-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/desktop-agent/tsconfig.app.json"
      }
    },
    "headless-agent": {
      "type": "application",
      "root": "apps/headless-agent",
      "entryFile": "main",
      "sourceRoot": "apps/headless-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/headless-agent/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "agent": {
      "type": "library",
      "root": "libs/agent",
      "entryFile": "index",
      "sourceRoot": "libs/agent/src",
      "compilerOptions": {
        "tsConfigPath": "libs/agent/tsconfig.lib.json"
      }
    },
    "electron": {
      "type": "library",
      "root": "libs/electron",
      "entryFile": "index",
      "sourceRoot": "libs/electron/src",
      "compilerOptions": {
        "tsConfigPath": "libs/electron/tsconfig.lib.json"
      }
    }
  }
}
