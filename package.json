{
  "name": "marketplace-agent",
  "version": "1.0.0",
  "main": "apps/desktop-agent/src/main.js",
  "author": {
    "name": "marketplace.nanox",
    "url": "https://marketplace.nanox.vision/",
    "email": "support@mdw.io"
  },
  "homepage": "https://mdw.io/",
  "repository": "https://github.com/MDWio/marketplace-agent-release",
  "description": "Client for Nanox Marketplace",
  "scripts": {
    "asar:extract:linux": "npm exec asar extract \"./release/linux-unpacked/resources/app.asar\" \"./asar-src\"",
    "asar:extract:mac-arm64": "npm exec asar extract \"./release/mac-arm64/Marketplace-Agent.app/Contents/Resources/app.asar\" \"./asar-src\"",
    "asar:extract:win": "npm exec asar extract \"./release/win-unpacked/resources/app.asar\" \"./asar-src\"",
    "desktop:build": "nest build agent && nest build core && nest build desktop-agent && npm run desktop:copy:assets",
    "desktop:build:win": "nest build agent && nest build core && nest build desktop-agent && npm run desktop:copy:assets:win",
    "desktop:clear": "rimraf dist/apps/desktop-agent",
    "desktop:copy:assets": "npx cpy 'apps/desktop-agent/src/assets/**/*' 'dist/apps/desktop-agent/src/assets'",
    "desktop:copy:assets:win": "npx cpy apps/desktop-agent/src/assets/**/* dist/apps/desktop-agent/src/assets",
    "desktop:db:generate": "npm exec electron ./dist/apps/desktop-agent/src/path.js && typeorm-ts-node-commonjs migration:generate -d dist/apps/desktop-agent/src/data-source.js",
    "desktop:db:migrate": "npm exec electron ./dist/apps/desktop-agent/src/path.js && typeorm-ts-node-commonjs migration:run -d dist/apps/desktop-agent/src/data-source.js",
    "desktop:db:revert": "npm exec electron ./dist/apps/desktop-agent/src/path.js && typeorm-ts-node-commonjs migration:revert -d dist/apps/desktop-agent/src/data-source.js",
    "desktop:start": "nest start desktop-agent -e \"npm exec electron ./dist/apps/desktop-agent/src/main.js\"",
    "desktop:start:dev": "npm run desktop:build && cross-env NODE_ENV=dev nest start -w desktop-agent -e \"npm exec electron ./dist/apps/desktop-agent/src/main.js\"",
    "electron:build:src": "npm run render:build && npm run desktop:build",
    "electron:build:src:win": "npm run render:build && npm run desktop:build:win",
    "electron:builder": "npm run rm:release && npm exec electron-builder build",
    "electron:linux": "npm run electron:build:src && npm run electron:builder -- --linux",
    "electron:mac": "npm run electron:build:src && npm run electron:builder -- --mac",
    "electron:windows": "npm run electron:build:src:win && npm run electron:builder -- --windows",
    "electron:publish": "npm exec electron-builder -- --publish always",
    "electron:windows:ci": "npm run electron:build:src && npm run rm:release && node ./node_modules/electron-builder/cli.js build --windows",
    "headless:build": "nest build agent && nest build core && nest build headless-agent",
    "headless:build:docker": "docker build --build-arg  GIT_COMMIT_HASH=$(git rev-parse HEAD) -t nanoxmarketplace/agent:$(node -p 'process.env.npm_package_version')_$(git rev-parse HEAD) .",
    "headless:clear": "rimraf dist/apps/headless-agent",
    "headless:db:generate": "typeorm-ts-node-commonjs migration:generate -d dist/apps/headless-agent/src/data-source.js",
    "headless:db:migrate": "typeorm-ts-node-commonjs migration:run -d dist/apps/headless-agent/src/data-source.js",
    "headless:db:revert": "typeorm-ts-node-commonjs migration:revert -d dist/apps/headless-agent/src/data-source.js",
    "headless:pm2:start": "pm2-runtime start ecosystem.config.js",
    "headless:start": "nest start headless-agent",
    "headless:start:dev": "nest start -w headless-agent",
    "ai-agent:build": "nest build ai-agent-lib && nest build core && nest build ai-agent",
    "ai-agent:build:docker": "set -x; docker build -f Dockerfile-ai --build-arg  GIT_COMMIT_HASH=$(git rev-parse HEAD) -t mdwio/ai-agent:${npm_package_version}_$(git rev-parse HEAD) .",
    "ai-agent:build:docker:win": "cross-env GIT_COMMIT_HASH=$(git rev-parse HEAD) TAG=mdwio/ai-agent:${npm_package_version}_$(git rev-parse HEAD) cross-env-shell \"docker build -f Dockerfile-ai -t $TAG --build-arg GIT_COMMIT_HASH=$GIT_COMMIT_HASH .\"",
    "ai-agent:clear": "rimraf dist/apps/ai-agent",
    "ai-agent:db:generate": "typeorm-ts-node-commonjs migration:generate -d dist/apps/ai-agent/src/data-source.js",
    "ai-agent:db:migrate": "typeorm-ts-node-commonjs migration:run -d dist/apps/ai-agent/src/data-source.js",
    "ai-agent:db:revert": "typeorm-ts-node-commonjs migration:revert -d dist/apps/ai-agent/src/data-source.js",
    "ai-agent:pm2:start": "pm2-runtime start ecosystem-ai.config.js",
    "ai-agent:start": "nest start ai-agent",
    "ai-agent:start:dev": "nest start -w ai-agent",
    "lint:all": "npm run lint:render && npm run lint:style && npm run lint:ai && npm run lint:desktop && npm run lint:libs && npm run lint:headless",
    "lint:all:ci": "npm run lint:render:ci && npm run lint:style:ci && npm run lint:desktop:ci && npm run lint:libs:ci && npm run lint:headless:ci",
    "lint:all:fix": "npm run lint:render:fix && npm run lint:style:fix && npm run lint:desktop:fix && npm run lint:libs:fix && npm run lint:headless:fix",
    "lint:desktop": "eslint --config \"./.eslintrc.json\" \"./apps/desktop-agent/**/*.ts\" ",
    "lint:desktop:ci": "npm run lint:desktop -- --format junit -o ./output/lint-desktop.xml --quiet",
    "lint:desktop:fix": "npm run lint:desktop -- --fix",
    "lint:ai": "eslint --config \"./.eslintrc.json\" \"./apps/ai-agent/**/*.ts\" ",
    "lint:ai:ci": "npm run lint:ai -- --format junit -o ./output/lint-ai.xml --quiet",
    "lint:ai:fix": "npm run lint:ai -- --fix",
    "lint:headless": "eslint --config \"./.eslintrc.json\" \"./apps/headless-agent/**/*.ts\" ",
    "lint:headless:ci": "npm run lint:headless -- --format junit -o ./output/lint-headless.xml --quiet",
    "lint:headless:fix": "npm run lint:headless -- --fix",
    "lint:libs": "eslint --config \"./.eslintrc.json\"  \"./libs/**/*.ts\"",
    "lint:libs:ci": "npm run lint:libs -- --format junit -o ./output/lint-libs.xml --quiet",
    "lint:libs:fix": "npm run lint:libs -- --fix",
    "lint:render": "ng lint",
    "lint:render:ci": "npm run lint:render -- --configuration=ci --quiet",
    "lint:render:fix": "npm run lint:render -- --fix",
    "lint:style": "stylelint --config \"./.stylelintrc.json\" \"./apps/render/app/**/*.(c|sa|sc)ss\"",
    "lint:style:ci": "npm run lint:style -- --custom-formatter 'node_modules/stylelint-junit-formatter' -s -o ./output/lint-style.xml",
    "lint:style:fix": "npm run lint:style -- --fix",
    "preai-agent:build": "npm run ai-agent:clear",
    "predesktop:build": "npm run desktop:clear",
    "predesktop:build:win": "npm run desktop:clear",
    "prerender:build": "npm run render:clear",
    "render:build": "ng build --base-href ./",
    "render:clear": "rimraf dist/apps/render",
    "render:start": "ng serve --port 4201",
    "render:watch": "ng build --base-href ./ --watch --configuration development",
    "rm:release": "rimraf release"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/platform-socket.io": "^9.3.10",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.10",
    "@svgdotjs/svg.js": "^3.1.2",
    "archiver": "^5.3.1",
    "axios": "^1.3.4",
    "axios-retry": "^3.4.0",
    "bignumber.js": "^9.1.1",
    "cron": "^2.2.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dcmjs": "^0.29.5",
    "dcmjs-dimse": "^0.1.17",
    "dicom-parser": "^1.8.21",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "join-images": "^1.1.5",
    "lodash": "^4.17.21",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "sharp": "^0.32.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "sqlite3": "^5.1.6",
    "svgdom": "^0.1.13",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.13",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/cli": "~15.2.4",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/compiler-cli": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/router": "^15.2.2",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@types/archiver": "^5.3.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.1",
    "@types/jest": "^29.4.4",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.15.3",
    "@types/node-forge": "^1.3.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cpy-cli": "^4.2.0",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "madge": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "rimraf": "^4.4.0",
    "stylelint": "^14.16.1",
    "stylelint-junit-formatter": "^0.2.2",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5",
    "zone.js": "~0.13.0"
  },
  "engines": {
    "node": "18.*",
    "npm": "9.*"
  }
}
